kenv(){
	eval $(minikube docker-env)
}
dconfig(){
	eval $(rm ~/.kube/config)
	eval $(ln -s ~/.kube/config.dev ~/.kube/config)
}
vconfig(){
	eval $(rm ~/.kube/config)
	eval $(ln -s ~/.kube/config.vagrant ~/.kube/config)
}
mkconfig(){
	eval $(rm ~/.kube/config)
	eval $(ln -s ~/.kube/config.minikube ~/.kube/config)
}
test -f ~/.git-completion.bash && . $_
